#!/bin/bash

source $SCRIPTS_DIR/include/functions
source $SCRIPTS_DIR/prepare	#nastavi promene a kontroluje existenci dulezitych slozek
source $SCRIPTS_DIR/include/ver #ulozi do promenych verze programu
source $SCRIPTS_DIR/include/l10n-EN

export WAIT=1
export ONLY_DL=0

waitUserUnpack (){
  if [ $WAIT == 1 ]; then
    echo "To unpack again type 0"
    echo "to continue pres enter"
    read repeat
  fi
}
export -f waitUserUnpack


NAME=${binutils[0]}
PROGRAM=${binutils[3]}
FILE=${binutils[2]}
EXT=${binutils[4]}
DLINK=${binutils[1]}
MD5=${binutils[5]}

if [ $ONLY_DL == 1 ]; then
		echo "$C only DL $DEF"
		download ${DLINK}
		check ${FILE} ${MD5}
		waitUser
	else
	repeat=0
	while [ $repeat == 0 ]; do
			download ${DLINK}
			check ${FILE} ${MD5}
			unpack ${EXT}
			SepBuild
			waitUserUnpack
	done
fi

NAME=${mpfr[0]}
PROGRAM=${mpfr[3]}
FILE=${mpfr[2]}
EXT=${mpfr[4]}
DLINK=${mpfr[1]}
MD5=${mpfr[5]}


if [ $ONLY_DL == 1 ]; then
		echo "$C only DL $DEF"
		download ${DLINK}
		check ${FILE} ${MD5}
		waitUser
	else
	repeat=0
	while [ $repeat == 0 ]; do
			download ${DLINK}
			check ${FILE} ${MD5}
			echo -e "$C Unpacking $DEF"
			echo -e "$G tar xjf $SOURCES_DIR/$FILE -C $BUILD_DIR/ $DEF"
			tar xjf $SOURCES_DIR/$FILE -C $BUILD_DIR/
			waitUserUnpack
	done
fi

download ${DLINK}
check ${FILE} ${MD5}


cd $ROOT

NAME=${gmp[0]}
PROGRAM=${gmp[3]}
FILE=${gmp[2]}
EXT=${gmp[4]}
DLINK=${gmp[1]}
