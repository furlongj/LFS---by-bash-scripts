MD5=${tar[5]}

if [ $ONLY_DL == 1 ]; then
		echo "$C only DL $DEF"
		download ${DLINK}
		check ${FILE} ${MD5}
		waitUser
	else
	repeat=0
	while [ $repeat == 0 ]; do
			download ${DLINK}
			check ${FILE} ${MD5}
			unpack ${EXT}
			waitUserUnpack
	done
fi

